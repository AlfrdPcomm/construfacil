{
  "flex-layout.row#categories": {
    "children": [
      "flex-layout.col#col-categories"
    ],
    "props": {
      "blockClass": ["centralizer","categories","line"]
    }
  },

  "flex-layout.col#col-categories":{
    "children":[
      "rich-text#title-categories",
      "flex-layout.row#categories-list",
      "link#all-categories"
    ],
    "props":{
      "preventVerticalStretch":true,
      "blockClass": ["categories-col"]
    }
  },

  "link#all-categories":{
    "props":{
      "label":"Todas las categor√≠as",
      "blockClass":"category-home-link"
    }
  },

  "rich-text#title-categories":{
    "props":{
      "text":"Tus compras seguras y faciles",
      "blockClass":"title-section",
      "textPosition":"CENTER",
      "textAlignment":"CENTER"
    }
  },

  "flex-layout.row#categories-list":{
    "children":[
      "flex-layout.col#categories1",
      "flex-layout.col#categories2",
      "flex-layout.col#categories3",
      "flex-layout.col#categories4",
      "flex-layout.col#categories5"
    ],
    "props":{
      "horizontalAlign": "center",
      "preventHorizontalStretch":true,
      "blockClass": ["categories-list"]
    }
  },

  "flex-layout.col#categories1": {
    "children": [
      "image#category1",
      "rich-text#category1"
    ],
    "props":{
      "blockClass": ["categories-subcol","categories-col-1"]
    }
  },
  "image#category1": {
    "props": {
      "src": "assets/categorias/categoria-acabados.png",
      "maxHeight": "220px",
      "blockClass":"img-categories",
      "link":{
        "url":"/"
      }
    }
  },
  "rich-text#category1": {
    "props": {
      "text": "## [acabados](/acabados)",
      "blockClass": "category-home",
      "textAlignment":"CENTER",
      "textPosition":"CENTER"
    }
  },
